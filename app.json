{
  "expo": {
    "name": "CrewCam",
    "slug": "GoldTeam",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android",
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/CC-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3D85C6",
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
    },
    "assetBundlePatterns": [
      "**/*",
    ],
    "ios": {
      "bundleIdentifier": "com.goldteam.crewcam",
      "supportsTablet": true,
    },
	"android": {
      "package": "com.goldteam.crewcam",
    }
  }
}